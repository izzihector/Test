<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_frontend" name="account assets" inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/nl_recruitment/static/src/css/announcements.css"/>
        </xpath>
    </template>

    <template id="announcement_detail" name="Job Detail" track="1">
        <t t-call="website.layout">
            <t  t-set="emp_types" t-value="{'open_ended': 'Open Ended', 'casual_contract': 'Casual and Service', 'fixed_term': 'Fixed Term', 'project_based': 'Project Based', 'fps1_2': 'FPS Category 1/2 Normal', 'fps1': 'FPS Category 2 Education', 'other': 'Other'}"/>
            <div class="announcement_main col-lg-7">
                <div class="__title">
                    <a role="button" t-attf-href="/jobs/announcements/apply/#{job.id}" class="btn float-right button">Apply Now</a>
                    <h4 class="__heading font-weight-bold col-lg-10 letter-spacing-sm">
                        <div><t t-esc="job.job_id.name"/></div>
                    </h4>
                </div>
                <div class="__company_info letter-spacing-sm"><small> <i class="fa fa-building" /> <t t-esc="job.company_id.name"/></small></div>
                <div class="dropdown-divider mt-4 mb-4"></div>
                <div class="__extra_info custom_color" >
                    <div class="row">
                        <div class="col-lg-2">Announcement Date:</div>
                        <div class="col-lg-4"><t t-esc="job.vacancy_start_date" /></div>
                        <div class="col-lg-2">Number of Vacancies:</div>
                        <div class="col-lg-4"><t t-esc="job.no_of_recruitment"/></div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2">Closing Date:</div>
                        <div class="col-lg-4"><t t-esc="job.vacancy_deadline" /></div>
                        <div class="col-lg-2">Requires Travel:</div>
                        <div class="col-lg-4">
                            <t t-if="job.requires_travel"> Yes </t>
                            <t t-else=""> No </t>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2">Duty Station:</div>
                        <div class="col-lg-4"><t t-esc="job.office_id.name" /></div>
                        <div class="col-lg-2">Contract Status:</div>
                        <div class="col-lg-4"><t t-esc="emp_types.get(job.employment_type, '')" /></div>
                    </div>
                    <div class="row">
                        <t t-set="all_gender" t-value="{'male': 'Male', 'female': 'Female' ,'male/female': 'Male/Female'}"/>
                        <div class="col-lg-2">Gender:</div>
                        <div class="col-lg-4"><t t-esc="all_gender.get(job.gender, '')" /></div>
                        <div class="col-lg-2">Probation Period:</div>
                        <t t-set="prob_period" t-value="{'one_month': '1 Month', 'two_month': '2 Month(s)' ,'three_month': '3 Month(s)'}"/>
                        <div class="col-lg-4">
                           <t t-if="job.probation_period">
                              <t t-esc="prob_period.get(job.probation_period, '')" />
                           </t>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2">VA Reference:</div>
                        <div class="col-lg-4"><t t-esc="job.va_reference" /></div>
                    </div>
                </div>
                <div class="__job_summary mt-4 mb-5 custom_color">
                    <div class="__job_summary_content pl-2">
                        <t t-raw="job.summary"/>                    
                    </div>
                </div>
                <div class="text-center">
                    <a role="button" t-attf-href="/jobs/announcements/apply/#{job.id}" class="btn button" >Apply Now</a>
                </div>
            </div>
        </t>
    </template>

    <template id="nl_recruit_my_academic_details">
        <style>
            label{
            font-size:15px;
            }
        </style>
        <div id="myModalAcademic" class="modal fade" role="dialog">
            <div class="modal-dialog modal-full-height" style="max-width:775px;">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">
                        Add Academic
                        Details
                    </h5>
                    <button type="button"
                        class="close"
                        data-dismiss="modal">
                    &amp;times;
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container-fluid" style="background-color:#ffffff">
                        <div class="row panel panel-default">
                        <div class="col-lg-12 panel-body">
                            <div id="infoEducation1">
                                <div class="row infoEducationGroup1">
                                    <div class="form-group col-lg-6">
                                    <label for="">
                                    Start Year <span style="color:#bd6d6d;">*</span>
                                    </label>
                                    <input
                                        type="date"
                                        name="start_year"
                                        class="form-control start_year_edu"
                                        />
                                    </div>
                                    <div class="form-group col-lg-6">
                                    <label for="">
                                    Completion Year <span style="color:#bd6d6d;">*</span>
                                    </label>
                                    <input
                                        type="date"
                                        name="completion_year"
                                        class="form-control completion_year_edu"
                                        />
                                    </div>
                                    <div class="form-group col-lg-6">
                                    <label for="">
                                    Degree <span style="color:#bd6d6d;">*</span>
                                    </label>
                                    <input
                                        type="text"
                                        id="degree"
                                        name="degree"
                                        class="form-control degree_edu">
                                    </input>
                                    </div>
                                    <div class="form-group col-lg-6">
                                    <label for="">
                                    Specialization <span style="color:#bd6d6d;"></span>
                                    </label>
                                    <input
                                        type="text"
                                        name="specialization"
                                        class="form-control specialization_edu"
                                        />
                                    </div>
                                    <div class="form-group col-lg-6">
                                    <label for="">
                                    University <span style="color:#bd6d6d;">*</span>
                                    </label>
                                    <input
                                        type="text"
                                        id="university"
                                        name="university"
                                        class="university_edu form-control">
                                    </input>
                                    </div>
                                    <div class="form-group col-lg-6">
                                    <input type="hidden" name="operation_type"/>
                                    <input type="hidden" name="opr_id"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button
                        type="button"
                        id="add_academic"
                        class="action-button1 add_academic btn">
                    Save
                    </button>
                    <button
                        type="button"
                        class="previous_button1 btn"
                        data-dismiss="modal">
                    Close
                    </button>
                </div>
            </div>
            </div>
        </div>
   </template>
   <template id="nl_recruit_my_experience_details">
      <div id="myModalExperience" class="modal fade" role="dialog">
         <div class="modal-dialog modal-full-height" style="max-width:775px;">
            <div class="modal-content">
               <div class="modal-header">
                  <h5 class="modal-title">
                     Add Experience Details
                  </h5>
                  <button type="button"
                     class="close"
                     data-dismiss="modal">
                  &amp;times;
                  </button>
               </div>
               <div class="modal-body">
                  <div class="container-fluid" style="background-color:#ffffff">
                     <div class="row panel panel-default">
